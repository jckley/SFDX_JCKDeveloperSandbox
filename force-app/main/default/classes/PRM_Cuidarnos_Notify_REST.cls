@RestResource(urlMapping='/cuidarnos/rest/notify/*')
global with sharing class PRM_Cuidarnos_Notify_REST {
    @HttpPost  
    global static void notifyUser() {
        RemotingMessageHelper.CountRecordsRespose objResponse = null;
        RemotingMessageHelper.CuidarnosNotifyUserRequest objRequest = null;
        RestRequest objRESTRequest = null;
        RestResponse objRESTResponse = null;
        List<Contact> lstContacts = null;
        List<Covid19__c> lstCovid19 = null;
        Set<String> setAllowedStates = null;
        String strSOQL = null;
        Map<String, String> mapGroupMap = null;
        Set<String> setConditions = null;
        
        System.debug('notifyUser [ request : ' + RestContext.request + ' ] -> ');
        
        try  {
            objResponse = new RemotingMessageHelper.CountRecordsRespose();
            objRequest = (RemotingMessageHelper.CuidarnosNotifyUserRequest) JSON.deserialize(RestContext.request.requestBody.toString(), RemotingMessageHelper.CuidarnosNotifyUserRequest.class);

            if(String.isNotBlank(objRequest.client) && (String.isNotBlank(objRequest.dni) || String.isNotBlank(objRequest.guid)  || String.isNotBlank(objRequest.salesforceid))) {
                objRESTRequest = RestContext.request;                
                objRESTResponse = RestContext.response;
                objRESTResponse.addHeader('Content-Type', 'application/json');
                setAllowedStates = retrieveAllowedNotifyStates();
                mapGroupMap = retrieveGroupMap();

                if(mapGroupMap.containsKey(objRequest.client)) {
                    strSOQL = 'SELECT Id, (SELECT Cuidarnos_Muestra_Result__c FROM Cuidarnos_Datas__r WHERE Cuidarnos_Muestra_Result__c IN :setAllowedStates) ';
                    strSOQL += ' FROM Contact ';
                    strSOQL += ' WHERE '; 
                    
                    if(String.isNotBlank(objRequest.dni)) {                
                        strSOQL += ' Document_Number__c = \'' + String.escapeSingleQuotes(objRequest.dni) + '\'';
                    }
                    
                    if(String.isNotBlank(objRequest.guid)) {
                        if(String.isNotBlank(objRequest.dni)) {
                            strSOQL += ' OR ';
                        }
                        strSOQL += ' guid__c = \'' + String.escapeSingleQuotes(objRequest.guid) + '\'';
                    }
                    
                    if(String.isNotBlank(objRequest.salesforceid)) {
                        if(String.isNotBlank(objRequest.dni) || String.isNotBlank(objRequest.guid)) {
                            strSOQL += ' OR ';
                        }

                        strSOQL += ' Id = \'' + String.escapeSingleQuotes(objRequest.salesforceid) + '\'';
                    }
                    System.debug('notifyUser [ strSOQL : ' + strSOQL + ' ]');

                    lstContacts = Database.query(strSOQL);

                    System.debug('notifyUser [ lstContacts : ' + lstContacts + ' ]');
                                    
                    if(lstContacts.size() == 1) {
                        if(lstContacts[0].Cuidarnos_Datas__r.size() == 1) {
                            //objResponse = new RemotingMessageHelper.CuidarnosCuitificacion(lstContacts[0]);
                            PRM_Cuidarnos.notifyUser(mapGroupMap.get(objRequest.client), lstContacts[0].Id);

                            objResponse.code = 0;
                            objResponse.message = 'Operación exitosa';
                            
                            objRESTResponse.statusCode = 200;
                            objRESTResponse.responseBody = Blob.valueOf(JSON.serialize(objResponse));   
                        } else {
                            objResponse.code = 130;
                            objResponse.message = 'No se puede notificar un usuario en el estado actual';
        
                            objRESTResponse.statusCode = 400; 
                            objRESTResponse.responseBody = Blob.valueOf(JSON.serialize(objResponse));   
        
                        }
                    } else if(lstContacts.size() > 1) {
                        objResponse.code = 120;
                        objResponse.message = 'Refine la busqueda. Se ha encontrado más de un registro para noficar';

                        objRESTResponse.statusCode = 400; 
                        objRESTResponse.responseBody = Blob.valueOf(JSON.serialize(objResponse));                  
                    } else {
                        objResponse.code = 110;
                        objResponse.message = 'No se han encontrado resultados';

                        objRESTResponse.statusCode = 400; 
                        objRESTResponse.responseBody = Blob.valueOf(JSON.serialize(objResponse));   
                    } 
                } else {
                    objResponse.code = 150;
                    objResponse.message = 'El cliente ingresado no esta habilitado para notificar';

                    objRESTResponse.statusCode = 400; 
                    objRESTResponse.responseBody = Blob.valueOf(JSON.serialize(objResponse));   
                }
            } else {
                objResponse.code = 100;
                objResponse.message = 'Información incompleta';

                objRESTResponse.statusCode = 400; 
                objRESTResponse.responseBody = Blob.valueOf(JSON.serialize(objResponse));   
            }
        } catch(Exception Ex) {
            objResponse = new RemotingMessageHelper.CountRecordsRespose();
            objResponse.code = 999;
            objResponse.message = Label.API_Invalid_Request;

            objRESTResponse.statusCode = 500; 
            objRESTResponse.responseBody = Blob.valueOf(JSON.serialize(objResponse));
        }
        
        System.debug('notifyUser [ statusCode : ' + objRESTResponse.statusCode + ' ]');
        System.debug('notifyUser [ responseBody : ' + objRESTResponse.responseBody + ' ]');
        System.debug('notifyUser [] <- ');
    }

    private static Set<String> retrieveAllowedNotifyStates() {
        Set<String> setAllowesStates = null;
        
        System.debug('retrieveAllowedNotifyStates [] ->');
        setAllowesStates = new Set<String>();
        setAllowesStates.add('No detectable');
        setAllowesStates.add('No Detectable');

        System.debug('retrieveAllowedNotifyStates [' + setAllowesStates + ' ] <- ');

        return setAllowesStates;
    }

    private static Map<String, String> retrieveGroupMap() {
        Map<String, String> mapGroupMap = null;
        
        System.debug('retrieveGroupMap [] ->');
        
        mapGroupMap = new Map<String, String>();
        mapGroupMap.put('pilar', 'Cuidarnos_Pilar');
        mapGroupMap.put('test','Cuidarnos_Test_Group');

        System.debug('retrieveGroupMap [' + mapGroupMap + ' ] <- ');

        return mapGroupMap;
    }

